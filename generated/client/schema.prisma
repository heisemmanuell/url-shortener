generator client {
  provider = "prisma-client-js"
  output   = "../generated/client"
}

datasource db {
  provider = "postgresql"
}

model Link {
  id          Int      @id @default(autoincrement())
  originalUrl String
  shortCode   String   @unique
  createdAt   DateTime @default(now())
  clicks      Int      @default(0)
  userId      String

  @@index([shortCode])
  @@index([userId])
}
